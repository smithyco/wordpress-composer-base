{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "johnpbloch/wordpress": "^4.6.1",
    "wpackagist-theme/twentysixteen": "*",
    "vlucas/phpdotenv": "^2.1",
    "wpackagist-plugin/relevanssi": "*",
    "wpackagist-plugin/wordpress-seo": "*",
    "wpackagist-plugin/wp-term-order": "*",
    "wpackagist-plugin/simple-image-sizes": "*",
    "wpackagist-plugin/google-sitemap-generator": "*",
    "wpackagist-plugin/tiny-compress-images": "*"
  },
  "require-dev": {
    "wpackagist-plugin/post-duplicator": "*",
    "wpackagist-plugin/database-sync": "*"
  },
  "extra": {
    "wordpress-install-dir": "public/core",
    "installer-paths": {
        "public/app/plugins/{$name}/": ["type:wordpress-plugin"],
        "public/app/themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "scripts":{
    "post-install-cmd": [

    ],
    "post-update-cmd": [
    ]
  }
}
